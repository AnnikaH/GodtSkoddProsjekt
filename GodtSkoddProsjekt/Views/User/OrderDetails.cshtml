@model List<GodtSkoddProsjekt.Models.Order>


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>OrderDetails</title>
</head>
<body>
    <div>
        <div class="col-sm-9">
            <h2 class="subpage_titles">Ordrehistorikk</h2>
            <hr />

            @foreach (var order in Model)
            {

                <div class="panel panel-default">
                    <div class="panel-heading">Ordre nr. @order.id</div>

                    <dl class="dl-horizontal">
                        <!--<dt>
                        Html.DisplayNameFor(model => model.userID)
                    </dt>

                    <dd>
                        Html.DisplayFor(model => model.userID)
                    </dd>-->

                        <dt>
                            @Html.DisplayNameFor(modelItem => order.date)
                        </dt>

                        <dd>
                            @Html.DisplayFor(modelItem => order.date)
                        </dd>

                        <dd>
                            @Html.DisplayFor(modelItem => order.orderlines[0])
                        </dd>

                    </dl>
                </div><!-- /.panel default-->
            }
        </div></div><!--./col-sm-9-->
</body>
</html>
